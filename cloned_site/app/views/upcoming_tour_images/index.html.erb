<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
<p style="color: green"><%= notice %></p>

<h1>Upcoming tour images</h1>

<div id="upcoming_tour_images">
  <% @upcoming_tour_images.each do |upcoming_tour_image| %>
    <%= render upcoming_tour_image %>
    <p>
      <%= link_to "Show this upcoming tour image", upcoming_tour_image %>
    </p>
  <% end %>
</div>

<%= link_to "New upcoming tour image", new_upcoming_tour_image_path %>
